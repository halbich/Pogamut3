<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE messageobject SYSTEM "../GBMessages.dtd">

<messageobject extends="InfoMessage" message="IUO" name="InteractedUsableObject" type="Event">
   <gbpackage name="GameBots2004"/>
   <gbpackage name="GBScenario"/>
   <gbpackage name="GameBotsAnim"/>
   <group name="Observer"/>
   <documentation>
		Asynchronous message. Sent when the observed player interacts with usable object.
    </documentation>
   <property name="Name" type="String" category="local">
      <documentation>
			Name of the usable object.
		</documentation>
   </property>
   <property name="Location" type="Location" category="local">
      <documentation>
			Location of the usable object.
		</documentation>
   </property>
   <property name="UOEvent" type="int" category="local">
      <documentation>
			Code of the action performed.
		</documentation>
   </property>
   <property name="Action" type="String" category="local">
      <documentation>
			Name of the action performed.
		</documentation>
   </property>
   <!-- Extra information used by different modules of the Pogamut platform. !--><extra>
      <code/>
   </extra>
</messageobject>