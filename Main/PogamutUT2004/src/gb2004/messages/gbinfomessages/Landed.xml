<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE messageobject SYSTEM "../GBMessages.dtd">

<messageobject extends="InfoMessage" message="LAND" name="Landed" type="Event">
   <gbpackage name="GameBots2004"/>
   <gbpackage name="GBScenario"/>
   <gbpackage name="GameBotsAnim"/>
   <gbpackage name="GameBotsUE2"/>
   <gbpackage name="GBEmohawk"/>
   <group name="Bot"/>
   <group name="Observer"/>
   <documentation>
		Asynchronous message. Sent when the bot lands on the ground after falling.
		Received also after respawn.
	</documentation>
   <!-- List of all properties of this message. !--><property name="HitNormal" type="Vector3d" category="local">
      <documentation>
			Normal of the plane we have landed on.
		</documentation>
   </property>
   <!-- Extra information used by different modules of the Pogamut platform. !--><extra>
      <code/>
   </extra>
</messageobject>