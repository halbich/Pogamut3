<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE messageobject SYSTEM "../GBMessages.dtd">

<messageobject extends="InfoMessage" message="LEFT" name="PlayerLeft" type="Event">
   <gbpackage name="GameBots2004"/>
   <gbpackage name="GBScenario"/>
   <gbpackage name="GameBotsAnim"/>
   <gbpackage name="GameBotsUE2"/>
   <gbpackage name="GBEmohawk"/>
   <group name="Bot"/>
   <group name="Server"/>
   <group name="Observer"/>
   <documentation>
		Asynchronous message. Message sent when a player leaves the
		server.
	</documentation>
   <!-- List of all properties of this message. !-->
   <property name="Id" type="UnrealId" category="all">
      <documentation>Unique Id of the player who left.</documentation>
   </property>
   <property name="Name" type="String" category="static">
      <documentation>The name of the player.</documentation>
   </property>
   <!-- Extra information used by different modules of the Pogamut platform. !--><extra>
      <code/>
   </extra>
</messageobject>